#quest√£o 1
SELECT 
  numero_departamento AS departamento
, AVG(salario)        AS media_salarial
FROM elmasri.funcionario
GROUP BY numero_departamento;

	 |departamento    |	media salarial
	 |     5	        | R$33.250,00 
   |  	 4	        | R$31.000,00 
	 |     1	        | R$55.000,00 

